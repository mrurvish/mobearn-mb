<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_home">


    <fragment
        android:id="@+id/navigation_home"
        android:name="com.mobearn.ad.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.mobearn.ad.ui.dashboard.DeshboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_deshboard" />


    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.mobearn.ad.ui.notifications.NotifactionFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifaction" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.mobearn.ad.ui.profile.ProfileFragment"
        android:label="bank"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/navigation_bank"
        android:name="com.mobearn.ad.ui.bank.BankFragment"
        android:label="bank"
        tools:layout="@layout/fragment_bank" />
</navigation>